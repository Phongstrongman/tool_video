# DouyinVoice Pro Server - Environment Variables
# Copy this file to .env and fill in your values

# ===== TIER-BASED API KEYS =====

# Basic Tier (50k VND, 100 videos/month)
GROQ_API_KEY_BASIC=your_groq_api_key_here
GEMINI_API_KEY_BASIC=your_gemini_api_key_here

# Pro Tier (150k VND, 500 videos/month)
# Use paid/premium API keys for better performance (optional - defaults to basic)
GROQ_API_KEY_PRO=your_paid_groq_key_here
GEMINI_API_KEY_PRO=your_paid_gemini_key_here

# VIP Tier (300k VND, unlimited videos)
# Use premium API keys with highest priority (optional - defaults to basic)
GROQ_API_KEY_VIP=your_premium_groq_key_here
GEMINI_API_KEY_VIP=your_premium_gemini_key_here

# Server Secret (REQUIRED - change in production!)
SERVER_SECRET_KEY=change-this-to-a-random-string-in-production

# Database
DATABASE_PATH=licenses.db

# Server Settings
HOST=0.0.0.0
PORT=8000

# CORS (comma-separated origins)
CORS_ORIGINS=*

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60

# License Settings
DEFAULT_LICENSE_DURATION_DAYS=30

# Logging
LOG_LEVEL=INFO
